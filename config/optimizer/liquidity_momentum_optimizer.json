    {"type": "Real", "name": "sell_thresh", "low": -1.0, "high": -0.1},
    {"type": "Integer", "name": "lookback_period", "low": 100, "high": 500},
    {"type": "Integer", "name": "momentum_period_1", "low": 5, "high": 20},
    {"type": "Integer", "name": "momentum_period_2", "low": 10, "high": 50},
    {"type": "Integer", "name": "momentum_period_3", "low": 20, "high": 100},
    {"type": "Categorical", "name": "exit_logic_name", "categories": ["trailing_stop", "fixed_stop", "atr_stop"]}
  ],
  "default_params": {
    "buy_thresh": 0.5,
    "sell_thresh": -0.5,
    "lookback_period": 252,
    "momentum_period_1": 5,
    "momentum_period_2": 10,
    "momentum_period_3": 20,
    "exit_logic_name": "trailing_stop"
  },
  "plot": true,
  "save_trades": true,
  "plot_size": [60, 30]
