{
  "data_dir": "data",
  "results_dir": "results",
  "strategy_name": "LiquidityMomentumStrategy",
  "symbol": "BTCUSDT",
  "interval": "4h",
  "start_date": "2023-01-01",
  "end_date": "2023-12-31",
  "initial_capital": 1000.0,
  "commission": 0.001,
  "risk_free_rate": 0.01,
  "omega_threshold": 0.0,
  "optimization_method": "optuna",
