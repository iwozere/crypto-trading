    "rsi": "cyan"
  },
  "report_metrics": ["total_trades", "win_rate", "sharpe_ratio"],
  "save_trades": true,
  "trades_csv_path": "results/trades.csv",
  "save_metrics": true,
  "metrics_format": "json",
  "print_summary": true,
  "report_params": true,
